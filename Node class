#ifndef CELULA_H
#define CELULA_H

class Celula {

	//= privado
	private:
  char info;
  Celula * prox;
  Celula * ant;
    
	//= publico
	public:
	Celula(char);
	char getInfo();
	void setInfo(char);
	Celula * getProx();
	Celula * getAnt();
	void setProx(Celula * );
	void setAnt(Celula * );
};
#endif
